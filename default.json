{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "groupName": "Qwer",
      "groupSlug": "qwer",
      "matchPackagePatterns": [
        "Qwer.*"
      ]
    },
    {
      "groupName": "protobuf-net",
      "groupSlug": "protobuf-net",
      "matchPackagePatterns": [
        "protobuf-net.*"
      ]
    },
    {
      "groupName": "NBitcoin",
      "groupSlug": "nbitcoin",
      "matchPackagePatterns": [
        "NBitcoin.*"
      ]
    },
    {
      "groupName": "Volo.Abp",
      "groupSlug": "volo",
      "matchPackagePatterns": [
        "Volo.Abp.*"
      ]
    },
    {
      "groupName": "Verify",
      "groupSlug": "verify",
      "matchPackagePatterns": [
        "Verify.*"
      ]
    },
    {
       "groupName":"EdjCase.ICP",
       "groupSlug":"edjCase-icp",
       "matchPackagePatterns": [
        "EdjCase.ICP.*"
      ]
    },
    {
       "groupName":"Nethereum",
       "groupSlug":"nethereum",
       "matchPackagePatterns": [
        "Nethereum.*"
      ]
    },
    {
       "groupName":"linq2db",
       "groupSlug":"linq2db",
       "matchPackagePatterns": [
        "linq2db.*"
      ]
    },
    {
       "groupName":"dbup",
       "groupSlug":"dbup",
       "matchPackagePatterns": [
        "dbup*"
      ]
    },
    {
       "groupName":"EasyCaching",
       "groupSlug":"EasyCaching",
       "matchPackagePatterns": [
        "EasyCaching*"
      ]
    }
  ]
}
